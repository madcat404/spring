@charset "utf-8";
main{width:100%; overflow: hidden; min-height:676px;}
.sub{overflow:hidden;}

/* header */
.header{position:fixed; width:100%; background:transparent; display:flex; justify-content:center; align-items:center; height:100px; z-index:999; transition:0.3s ease-in-out;}
.header__inner{width:100%; height:100%; position:relative; max-width:1200px; background:transparent; display:flex; justify-content:space-between; align-items:center;}
.header .black_icon{display:none;}
.header__logo a{display:flex; background:url('/images/assets/logo_white.png') no-repeat center; background-size:cover; width:128px; height:36px;}
.header__gnb{height:100%;}
.header__gnb ul li a {color:#222; font-weight:500; font-size:16px;  letter-spacing: 0;}
.header__gnb > ul{display:flex; height:100%;}
.header__gnb > ul > li{margin-right:80px; position:relative; height:100%; display:flex; align-items:center;}
.header__gnb > ul > li:last-child{margin-right:0;}
.header__gnb > ul > li > a {color:#fff; position:relative;  }
.header__gnb > ul > li > a:after{content:''; width:0; height:2px; background:#fff; transition:all ease .3s; position:absolute;  top:-10px; left:50%;}
.header__gnb > ul > li:hover a:after{width:100%; left:0;}
.header__gnb .gnb__2depth{position:absolute; width:210px; top:80%; left:50%; margin-left:-106px; height:0; opacity:0; background:rgba(255,255,255,0.8); box-shadow:2px 2px 5px rgba(0,0,0,0.05); text-align:center;   transition:opacity ease .3s; overflow:hidden; border-radius:4px;}
.header__gnb .gnb__2depth li{}
.header__gnb .gnb__2depth li a{font-weight:500; font-size:14px; white-space:nowrap; padding:7px 0;   display:block;  transition:all ease .6s;}
.header__gnb .gnb__2depth li:last-child a{border-bottom:0;}
.header__gnb .gnb__2depth li:hover a{color:#004e99;}
.header__gnb ul li:hover .gnb__2depth{opacity:1; height:auto; padding: 20px ;  border:1px solid #d9d9d9; }
.header__lan{display:flex; align-items:center; position:relative; cursor:pointer;}
.header__lan .blue_icon{display:none;}
.header__lan img{max-width:16px; max-height:16px; margin-right:14px;}
.header__lan .lan__selected{color:#fff; font-size:12px; cursor:pointer; line-height:1; padding:10px 30px;   border-radius:100px; font-weight:700; border:1px solid #fff; transition:all ease .3s;}
.header__lan:hover .lan__2depth{display:block;}
.header__lan .lan__2depth{position:absolute; top:100%; left:30px; display:none; background:#fff;}
.header__lan .lan__2depth li{padding:10px 0;}
.header__lan .lan__2depth li a {color:#fff; font-size:14px; font-weight:500;}
.header__lan .lan__2depth li a:hover{color:#004e99;}
.header .sns_wrap a {position:absolute; right:5%; top:50%; transform:translate3d(0,-50%,0); width:67px; height:15px; background:url(/images/assets/youtube_white.png) no-repeat center; background-size:cover;}

/* header scroll down */
.header--solid{background:#fff;  height:70px;}
.header--solid .header__logo a:first-child{display:flex; background:url('/images/assets/logo.png') no-repeat center; background-size:cover;}
.header--solid .white_icon{display:none;}
.header--solid .black_icon{display:block;}
.header--solid .header__gnb ul li a {color:#222;}
.header--solid .header__gnb > ul > li > a:after{background:#004e99}
.header--solid .header__lan .lan__selected {border:1px solid #333; color:#333; background:transparent;}
.header--solid .header__gnb .gnb__2depth{top:85%}
.header--solid .sns_wrap a {background:url(/images/assets/youtube.png) no-repeat center; background-size:cover;}

@media screen and (max-width: 1440px) {
    .header .sns_wrap,
    .header--solid .sns_wrap {display:none;}
}


